pipeline{
    agent any
    stages{
        stage("Hello stage"){
            steps{
                echo "Hello Jay! Good Morning!"
            }
        }
        stage("Bye Bye"){
            steps{
                echo "Bye Bye Jay! Have a nice day!"
            }
        }
	stage("Git clone"){
	    steps{
		sh 'git clone https://github.com/jaysurya1697/hello-world.git'
		sh 'git remote add origin https://github.com/jaysurya1697/hello-world.git'
		sh 'ls -lrt'
	    }
	}
}
}
