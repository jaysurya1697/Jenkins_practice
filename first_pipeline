pipeline{
    agent any
    stages{
        stage("Hello stage"){
            steps{
                echo "Hello Jay! Good Morning!"
            }
        }
        stage("Bye Bye"){
            steps{
                echo "Bye Bye Jay! Have a nice day!"
            }
        }
	stage("Git clone"){
	    steps{
		sh 'sudo rm -rf hello-world'
		sh 'sudo git clone https://github.com/jaysurya1697/hello-world.git'
		sh 'pwd'
		sh 'ls -lrt'
		sh 'cd hello-world/'
		sh 'pwd'
		sh 'ls -lrt'
	    }
	}
}
}
