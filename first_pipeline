pipeline{
    agent any
    stages{
        stage("Hello stage"){
            steps{
                echo "Hello Jay! Good Morning!"
            }
        }
        stage("Bye Bye"){
            steps{
                echo "Bye Bye Jay! Have a nice day!"
            }
        }
	stage("Git clone"){
	    steps{
		git clone https://github.com/jaysurya1697/hello-world.git
		git remote add origin https://github.com/jaysurya1697/hello-world.git
		ls -lrt
	    }
	}
}
}
